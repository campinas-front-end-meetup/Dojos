{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    modules: true
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": [
      2,
      "function"
    ],
    "no-eq-null": 2,
    "no-eval": 2,
    "no-unused-expressions": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-multiple-empty-lines": [
      2,
      { "max": 1 }
    ],
    "no-console": 1,
    "keyword-spacing": [
      2,
      { "before": true }
    ],
    "space-in-parens": 2,
    "space-before-blocks": 2,
    "key-spacing": 2,
    "space-infix-ops": 2,
    "semi": 2,
    "max-len": [2, 100, 2]
  },
  "globals": {
    "it": true,
    "describe": true,
    "process": true
  }
}
